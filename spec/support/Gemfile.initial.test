source 'http://rubygems.org'
ruby '1.9.3'

gem 'rails', '3.2.8'
gem "pg"
gem "nokogiri"
gem 'jquery-rails'
gem "airbrake"
gem "devise"
gem "twitter-bootstrap-rails", "2.1.4"
gem "friendly_id"
gem "redcarpet"
gem "rack-timeout"
gem "mixpanel"
gem "rails3_acts_as_paranoid", "~> 0.2.0"


group :development, :test do
  gem "rspec-rails"
  gem "factory_girl_rails"
end

group :assets do
  gem 'sass-rails',   '~> 3.2.3'
  gem 'uglifier', '>= 1.0.3'
end

